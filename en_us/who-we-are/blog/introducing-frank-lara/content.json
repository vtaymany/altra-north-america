            "grid_column": {
              "column-content": {
                "image": {
                  "image": {
                    "_cq:lastRolledout": "{Date}2023-01-25T22:57:34.890Z",
                    "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
                    "_jcr:mixinTypes": "[cq:LiveRelationship]",
                    "_jcr:primaryType": "nt:unstructured",
                    "_alt": "Headshot of Frank Lara",
                    "_autoCalcSize": "autosize",
                    "_desktopImageUrl": "https://images.altrarunning.com/is/image/altra/Frank%5FLara%5FAnnouncement%5FBlog%5FImage%5F1?"
                  },
                  "_cq:lastRolledout": "{Date}2023-01-25T22:57:34.875Z",
                  "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
                  "_jcr:created": "{Date}2021-10-14T14:56:04.121Z",
