                  "_paddingBottom": "bottom-pad-default",
                  "_paddingTop": "top-pad-default",
                  "_text": "<h1 style=\"text-align: center;\">CONGRATULATIONS 2023 HARDROCK 100 FINISHERS!&nbsp;</h1>\r\n",
                  "_textIsRich": "true"
                },
                "text_copy": {
                  "_cq:lastRolledout": "{Date}2023-07-21T15:31:17.477Z",
