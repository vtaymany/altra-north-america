                    <grid_column_copy
                        cq:lastRolledout="{Date}2023-01-25T22:47:34.448Z"
                        cq:lastRolledoutBy="evan_lynch@vfc.com"
                        jcr:created="{Date}2021-02-22T22:50:02.400Z"
                        jcr:createdBy="abbey_charles@vfc.com"
                        jcr:lastModified="{Date}2023-01-25T22:47:34.440Z"
                        jcr:lastModifiedBy="abbey_charles@vfc.com"
                        jcr:mixinTypes="[cq:LiveRelationship]"
                        jcr:primaryType="nt:unstructured"
                        sling:resourceType="vfcorp/components/global/content/grid_column"
                        largeScreensCenterColumn="true"
