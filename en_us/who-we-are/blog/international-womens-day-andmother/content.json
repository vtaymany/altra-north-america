                "text_2040045251": {
                  "_cq:lastRolledout": "{Date}2023-01-25T22:39:44.009Z",
                  "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
                  "_jcr:created": "{Date}2021-03-05T21:35:37.038Z",
