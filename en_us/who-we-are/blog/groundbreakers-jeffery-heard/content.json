                  "_customCSS": ".space {\r\n    height: 4rem;\r\n}\r\n\r\n.grey-background-groundbreakers-blog {\r\n    background-color: #f2f2f2;\r\n    padding-left: 4rem;\r\n    padding-right: 4rem;\r\n}",
                  "_customHTML": "<div class=\"space\"></div>",
                  "_marginBottom": "bottom-margin-default",
