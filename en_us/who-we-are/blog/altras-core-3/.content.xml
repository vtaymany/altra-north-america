                                    jcr:mixinTypes="[cq:LiveRelationship]"
                                    jcr:primaryType="nt:unstructured"
                                    alt="Foot Shape Toe Box - What you can't see matters most"
                                    autoCalcSize="autosize"
                                    desktopImageUrl="https://images.altrarunning.com/is/image/altra/foot%2Dspace?$hero%2Dfullbleed%2Dmobile$"/>
                            </image>
                            <customhtml2_copy_cop
                                cq:lastRolledout="{Date}2023-01-25T22:00:49.921Z"
                                cq:lastRolledoutBy="evan_lynch@vfc.com"
                                jcr:created="{Date}2018-10-08T10:43:53.923-04:00"
                                jcr:createdBy="jessica.sarkis@sapientrazorfish.com"
                                jcr:lastModified="{Date}2023-01-25T22:00:49.915Z"
                                jcr:lastModifiedBy="wade_litsey@vfc.com"
                                jcr:mixinTypes="[cq:LiveRelationship]"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="vfcorp/components/global/content/customhtml2"
                                customHTML="&lt;br>&#xd;&#xa;&lt;h3>Benefits of FootShape™ Toe Box&lt;/h3>&#xd;&#xa;&lt;ul>&lt;li>Keeps big toe straight for greater stability and a more powerful toe-off&lt;/li>&#xd;&#xa;&lt;li>Allows toes to relax and spread out naturally&lt;/li>&#xd;&#xa;&lt;li>Allows foot to naturally stabilize excess pronation&lt;/li>&lt;/ul>&#xd;&#xa;&lt;br>&#xd;&#xa;&lt;br>&#xd;&#xa;"
                                marginBottom="bottom-margin-default"
                                marginTop="top-margin-default"
                                paddingBottom="bottom-pad-default"
                                paddingTop="top-pad-default"/>
                            <customhtml2_copy_cop_1200856536
                                cq:lastRolledout="{Date}2023-01-25T22:00:49.932Z"
                                cq:lastRolledoutBy="evan_lynch@vfc.com"
                                jcr:created="{Date}2018-10-08T10:43:53.923-04:00"
                                jcr:createdBy="jessica.sarkis@sapientrazorfish.com"
                                jcr:lastModified="{Date}2023-01-25T22:00:49.926Z"
                                jcr:lastModifiedBy="wade_litsey@vfc.com"
                                jcr:mixinTypes="[cq:LiveRelationship]"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="vfcorp/components/global/content/customhtml2"
                                customHTML="&lt;br>&#xd;&#xa;&lt;h2>Zero Drop™ Platform&lt;/h2>&#xd;&#xa;&lt;p>Every Altra running shoe features a fully cushioned Zero Drop™ platform that places the heel and forefoot at the same distance from the ground. This natural balance aligns the feet, back and body posture for less impact. It also strengthens the Achilles and lower calf muscles that have been weakened over a lifetime of running on elevated heels.&lt;/p>&#xd;&#xa;"
                                marginBottom="bottom-margin-default"
                                marginTop="top-margin-default"
                                paddingBottom="bottom-pad-default"
                                paddingTop="top-pad-default"/>
                            <image_1118135373
                                cq:lastRolledout="{Date}2023-01-25T22:00:49.940Z"
                                cq:lastRolledoutBy="evan_lynch@vfc.com"
                                jcr:created="{Date}2019-03-05T18:28:37.306-05:00"
                                jcr:createdBy="wade_litsey@vfc.com"
