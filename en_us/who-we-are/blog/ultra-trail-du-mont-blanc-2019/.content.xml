                            <text
                                cq:lastRolledout="{Date}2023-01-25T22:04:46.615Z"
                                cq:lastRolledoutBy="evan_lynch@vfc.com"
                                jcr:created="{Date}2019-08-23T16:56:50.855Z"
                                jcr:createdBy="wade_litsey@vfc.com"
                                jcr:lastModified="{Date}2023-01-25T22:04:46.609Z"
