        "image": {
          "image": {
            "_cq:lastRolledout": "{Date}2023-01-25T22:06:48.427Z",
            "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
            "_jcr:mixinTypes": "[cq:LiveRelationship]",
            "_jcr:primaryType": "nt:unstructured",
            "_alt": "Michael McKnight achieved the Triple Crown of 200s more than once",
            "_autoCalcSize": "autosize",
            "_desktopImageUrl": "https://images.altrarunning.com/is/image/altra/mike%2Dmcknight%2Dtriple%2Dcrown%2Dof%2D200s?$hero%2Dfullbleed%2Ddesktop$"
          },
          "_cq:lastRolledout": "{Date}2023-01-25T22:06:48.412Z",
          "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
