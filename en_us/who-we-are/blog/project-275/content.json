                  "_jcr:created": "{Date}2022-02-09T21:30:16.901Z",
                  "_jcr:createdBy": "dylan_gibson@vfc.com",
                  "_jcr:lastModified": "{Date}2023-03-07T18:37:51.887Z",
                  "_jcr:lastModifiedBy": "dylan_gibson@vfc.com",
                  "_jcr:mixinTypes": "[cq:LiveRelationship]",
                  "_jcr:primaryType": "nt:unstructured",
                  "_sling:resourceType": "vfcorp/components/global/content/image",
