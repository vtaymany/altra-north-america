                    "_autoCalcSize": "autosize",
                    "_desktopImageUrl": "https://images.altrarunning.com/is/content/altra/Marketing%20Assets/Website/Landing%20Pages/220210%20%2D%20Project%20275/03%5FProject275%5FLanding%5FPage%5FComfort.gif"
                  },
                  "_cq:lastRolledout": "{Date}2023-03-07T18:37:51.917Z",
                  "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
                  "_jcr:created": "{Date}2022-02-09T21:38:22.267Z",
