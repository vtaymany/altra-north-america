                    "_jcr:mixinTypes": "[cq:LiveRelationship]",
                    "_jcr:primaryType": "nt:unstructured",
                    "_autoCalcSize": "autosize",
                    "_desktopImageUrl": "https://images.altrarunning.com/is/content/altra/Marketing%20Assets/Website/Landing%20Pages/220210%20%2D%20Project%20275/02%5FProject275%5FLanding%5FPage%5FInnovation.gif"
