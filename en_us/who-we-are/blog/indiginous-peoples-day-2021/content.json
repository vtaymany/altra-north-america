                  "_paddingBottom": "bottom-pad-default",
                  "_paddingTop": "top-pad-default",
                  "_text": "<h3>&nbsp;</h3>\r\n<h3>&nbsp;</h3>\r\n<h3>HONORING THE PAST</h3>\r\n",
                  "_textIsRich": "true"
                },
                "text_1365038991": {
                  "_cq:lastRolledout": "{Date}2023-01-25T22:57:25.282Z",
                  "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
                  "_jcr:created": "{Date}2021-10-11T15:37:36.765Z",
                  "_jcr:createdBy": "abbey_charles@vfc.com",
                  "_jcr:lastModified": "{Date}2023-01-25T22:57:25.275Z",
                  "_jcr:lastModifiedBy": "abbey_charles@vfc.com",
                  "_jcr:mixinTypes": "[cq:LiveRelationship]",
                  "_jcr:primaryType": "nt:unstructured",
                  "_sling:resourceType": "vfcorp/components/global/content/text",
                  "_marginBottom": "bottom-margin-default",
                  "_marginTop": "top-margin-default",
                  "_paddingBottom": "bottom-pad-default",
                  "_paddingTop": "top-pad-default",
                  "_text": "<p><b>Recognizing Indigenous existence and honoring Ancestral lives and their sacrifices.</b></p>\r\n",
                  "_textIsRich": "true"
                },
                "text_1028842045_copy": {
                  "_cq:lastRolledout": "{Date}2023-01-25T22:57:25.295Z",
                  "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
