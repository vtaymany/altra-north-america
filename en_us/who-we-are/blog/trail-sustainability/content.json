                  "_text": "<h1>&nbsp;</h1>\r\n<h1>Trail Sustainability Tips for the San Juan Mountains&nbsp;</h1>\r\n",
                  "_textIsRich": "true"
                },
                "text_copy": {
                  "_cq:lastRolledout": "{Date}2023-01-25T23:07:24.967Z",
                  "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
                  "_jcr:created": "{Date}2021-12-16T21:45:16.329Z",
