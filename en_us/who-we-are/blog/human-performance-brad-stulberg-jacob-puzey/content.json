                    "_videoType": "youtube"
                  },
                  "_cq:lastRolledout": "{Date}2023-01-25T22:26:40.661Z",
                  "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
                  "_jcr:created": "{Date}2020-10-28T19:29:39.856Z",
                  "_jcr:createdBy": "abbey_charles@vfc.com",
                  "_jcr:lastModified": "{Date}2023-01-25T22:26:40.657Z",
                  "_jcr:lastModifiedBy": "abbey_charles@vfc.com",
                  "_jcr:mixinTypes": "[cq:LiveRelationship]",
