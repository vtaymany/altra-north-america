                            <text_copy_1853875258
                                cq:lastRolledout="{Date}2023-06-20T22:56:37.317Z"
                                cq:lastRolledoutBy="evan_lynch@vfc.com"
                                jcr:created="{Date}2021-02-22T22:44:53.916Z"
                                jcr:createdBy="abbey_charles@vfc.com"
                                jcr:lastModified="{Date}2023-06-20T22:56:37.312Z"
                                jcr:lastModifiedBy="evan_lynch@vfc.com"
                                jcr:mixinTypes="[cq:LiveRelationship]"
