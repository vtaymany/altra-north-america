                                    autoCalcSize="autosize"
                                    desktopImageUrl="https://images.altrarunning.com/is/image/altra/23F%5FParadigm7%5FKaraGoucher%5FImage%2D1%5F1920x1080?"/>
                            </image>
                            <customhtml2_copy_cop_1061093371
                                cq:lastRolledout="{Date}2023-06-20T22:56:37.352Z"
                                cq:lastRolledoutBy="evan_lynch@vfc.com"
                                jcr:created="{Date}2020-12-22T17:49:16.560Z"
