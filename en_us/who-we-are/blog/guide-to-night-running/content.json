            "grid_column_434783644": {
              "column-content": {
                "text": {
                  "_cq:lastRolledout": "{Date}2023-01-25T22:58:42.092Z",
                  "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
                  "_jcr:created": "{Date}2021-11-19T19:20:48.748Z",
