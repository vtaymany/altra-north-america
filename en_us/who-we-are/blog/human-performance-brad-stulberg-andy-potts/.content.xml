                            </image_copy_copy_copy>
                            <text_71609347
                                cq:lastRolledout="{Date}2023-01-25T22:35:47.994Z"
                                cq:lastRolledoutBy="evan_lynch@vfc.com"
                                jcr:created="{Date}2020-10-22T19:53:21.155Z"
