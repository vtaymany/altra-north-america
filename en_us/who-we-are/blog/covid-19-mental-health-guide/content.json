                  "_sling:resourceType": "vfcorp/components/global/content/text",
                  "_text": "<h1 style=\"text-align: center;\">A MENTAL HEALTH GUIDE FOR ATHLETES DURING COVID-19</h1>\r\n",
                  "_textIsRich": "true"
                },
                "_cq:lastRolledout": "{Date}2023-01-25T22:13:36.337Z",
                "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
