            "_alt": "A Mental Health Guide for COVID-19",
            "_autoCalcSize": "preset",
            "_desktopImageUrl": "https://images.altrarunning.com/is/image/altra/covid%2D19%2Dmental%2Dhealth%2Dguide"
          },
          "_cq:lastRolledout": "{Date}2023-01-25T22:13:36.514Z",
          "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
          "_jcr:created": "{Date}2020-03-27T00:36:01.208Z",
          "_jcr:createdBy": "wade_litsey@vfc.com",
          "_jcr:lastModified": "{Date}2023-01-25T22:13:36.510Z",
          "_jcr:lastModifiedBy": "wade_litsey@vfc.com",
