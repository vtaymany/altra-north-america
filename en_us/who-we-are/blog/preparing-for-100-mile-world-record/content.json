            "_jcr:mixinTypes": "[cq:LiveRelationship]",
            "_jcr:primaryType": "nt:unstructured",
            "_alt": "Zach Bitter prepares for 100 mile world record",
            "_autoCalcSize": "autosize",
            "_desktopImageUrl": "https://images.altrarunning.com/is/image/altra/zach%2Dbitter%2Dprepares%2Dfor%2Dworld%2Drecord?$hero%2Dfullbleed%2Ddesktop$"
          },
          "_cq:lastRolledout": "{Date}2023-01-25T22:06:25.641Z",
          "_cq:lastRolledoutBy": "evan_lynch@vfc.com",
          "_jcr:created": "{Date}2019-09-24T17:01:59.786Z",
          "_jcr:createdBy": "wade_litsey@vfc.com",
          "_jcr:lastModified": "{Date}2023-01-25T22:06:25.636Z",
          "_jcr:lastModifiedBy": "wade_litsey@vfc.com",
          "_jcr:mixinTypes": "[cq:LiveRelationship]",
