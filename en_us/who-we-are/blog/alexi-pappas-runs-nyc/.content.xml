                                jcr:mixinTypes="[cq:LiveRelationship]"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="vfcorp/components/global/content/image"
                                float="none">
                                <image
                                    cq:lastRolledout="{Date}2023-01-25T22:58:11.579Z"
                                    cq:lastRolledoutBy="evan_lynch@vfc.com"
                                    jcr:mixinTypes="[cq:LiveRelationship]"
                                    jcr:primaryType="nt:unstructured"
                                    alt="Alexi Pappas with her medal from the New York Marathon"
                                    autoCalcSize="autosize"
                                    desktopImageUrl="https://images.altrarunning.com/is/image/altra/Alexi%5FPappas%5FNYC%5FMarathon%5FBlog%5FImage%5F2?"/>
                            </image_1004881933>
                            <text_711941125
                                cq:lastRolledout="{Date}2023-01-25T22:58:11.591Z"
                                cq:lastRolledoutBy="evan_lynch@vfc.com"
                                jcr:created="{Date}2021-11-12T22:29:34.895Z"
                                jcr:createdBy="abbey_charles@vfc.com"
                                jcr:lastModified="{Date}2023-01-25T22:58:11.584Z"
                                jcr:lastModifiedBy="abbey_charles@vfc.com"
                                jcr:mixinTypes="[cq:LiveRelationship]"
                                jcr:primaryType="nt:unstructured"
                                sling:resourceType="vfcorp/components/global/content/text"
                                marginBottom="bottom-margin-default"
                                marginTop="top-margin-default"
                                paddingBottom="bottom-pad-default"
                                paddingTop="top-pad-default"
                                text="&lt;p>&amp;nbsp;&lt;/p>&#xd;&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xd;&#xa;&lt;p>&lt;u>&lt;b>About the Author&lt;/b>&lt;/u>&lt;/p>&#xd;&#xa;&lt;p>Alexi Pappas is an Altra Elite Team member. She resides in Alameda, California. She has recently published her first book - Bravery - and is looking forward to telling her story with Altra Running. Follow her on &lt;a href=&quot;https://twitter.com/AlexiPappas&quot;>Twitter&lt;/a> and &lt;a href=&quot;https://www.instagram.com/alexipappas/&quot;>Instagram&lt;/a>: @alexipappas&lt;/p>&#xd;&#xa;"
                                textIsRich="true"/>
                        </column-content>
                    </grid_column>
                </row-content>
