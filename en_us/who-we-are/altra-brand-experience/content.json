          "_customJS": "//Hero play/pause logic\r\nwindow.addEventListener('DOMContentLoaded', function () {\r\n  var video = document.getElementById('hero-video');\r\n  var image = document.getElementById('hero-video-status-icon');\r\n\r\n  // Function to handle the play/pause button click\r\n  function togglePlayPause() {\r\n    if (video.paused) {\r\n      video.play();\r\n      image.src =\r\n        'https://images.jansport.com/is/image/JanSportBrand/altra-difference-pause-button-07062023?fmt=png-alpha';\r\n      image.alt = 'pause button';\r\n    } else {\r\n      video.pause();\r\n      image.src =\r\n        'https://images.jansport.com/is/image/JanSportBrand/altra-difference-play-button-07062023?fmt=png-alpha';\r\n      image.alt = 'play button';\r\n    }\r\n  }\r\n\r\n  // Attach click event listener to the play/pause button\r\n  video.addEventListener('click', togglePlayPause);\r\n});\r\n\r\nrequire(['jquery', 'domReady'], function ($, domReady) {\r\n  domReady(function () {\r\n    //Start fade animation\r\n    function checkElementLocation() {\r\n      var $window = $(window);\r\n      var bottom_of_window = $window.scrollTop() + $window.height();\r\n\r\n      $('.fade-elem-1').each(function (i) {\r\n        var $that = $(this);\r\n        var bottom_of_object = $that.position().top + $that.outerHeight();\r\n\r\n        //if element is in viewport, add the animate class\r\n        if (bottom_of_window > bottom_of_object) {\r\n          $(this).addClass('fade-in');\r\n        }\r\n      });\r\n    }\r\n    // if in viewport, show the animation\r\n    checkElementLocation();\r\n\r\n    $(window).on('scroll', function () {\r\n      checkElementLocation();\r\n    });\r\n    //End fade animation\r\n  });\r\n});\r\n\r\nrequire(['jquery', 'domReady'], function ($, domReady) {\r\n  domReady(function () {\r\n    //Start fade animation\r\n    function checkElementLocation() {\r\n      var $window = $(window);\r\n      var bottom_of_window = $window.scrollTop() + $