                  "_customCSS": "\r\n.family-product-comparison-table td:first-of-type {\r\n    font-weight: bold;\r\n    text-align: left;\r\n    padding-left: 2%;\r\n}\r\n.family-product-comparison-table thead,\r\n.family-product-comparison-table tbody,\r\n.family-product-comparison-tablet foot,\r\n.family-product-comparison-table table th,\r\n.family-product-comparison-table table td {\r\n  border: 0;\r\n}\r\n\r\n.family-product-comparison-table table th,\r\n.family-product-comparison-table table td {\r\n    border-bottom: 1px solid #666666 !important;\r\n    padding: 20px 0;\r\n}",
                  "_customHTML": "<div class=\"family-product-comparison-table\" style=\"overflow-x:auto;\">\r\n  <table>\r\n    <tr>\r\n      <th></th>\r\n      <th>MONT BLANC</th>\r\n      <th>MONT BLANC BOA®</th>\r\n    </tr>\r\n    <tr>\r\n      <td>EGO&trade; MAX Foam</td>\r\n      <td>Yes</td>\r\n      <td>Yes</td>\r\n    </tr>\r\n     <tr>\r\n      <td><a href=\"https://www.altrarunning.com/who-we-are/footshape.html\" style=\"text-decoration: underline\">Standard FootShape&trade; Fit</a></td>\r\n      <td>Yes</td>\r\n      <td>Yes</td>\r\n    </tr>\r\n     <tr>\r\n      <td>Vibram® Litebase</td>\r\n      <td>Yes</td>\r\n      <td>Yes</td>\r\n    </tr>\r\n    <tr>\r\n      <td>Weight (oz)</td>\r\n      <td>6.9 W/9.9 M</td>\r\n      <td>9.3 W/11.2 M</td>\r\n    </tr>\r\n    <tr>\r\n      <td>Stack Height (mm)</td>\r\n      <td>30</td>\r\n      <td>30</td>\r\n    </tr>\r\n    <tr>\r\n      <td>Upper Details</td>\r\n      <td>Ultra-lightweight</br> & breathable</td>\r\n      <td>Ultra-lightweight</br> & breathable</td>\r\n    </tr>\r\n     <tr>\r\n      <td>Lacing System</td>\r\n      <td>Traditional</td>\r\n      <td>PerformFit&trade; Wrap, powered</br> by the BOA® Fit System</td>\r\n    </tr>\r\n  </table>\r\n</div>\r\n<div class=\"space\"></div>",
